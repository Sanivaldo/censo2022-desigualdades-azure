{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-censo-data-pipelines-2025"
		},
		"ls_adls_censo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_censo'"
		},
		"ls_adls_censo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stcensodados.dfs.core.windows.net/"
		},
		"ls_ibge_sidra_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sidra.ibge.gov.br/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-censo-data-pipelines-2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_censo_accountKey": {
						"value": "[parameters('ls_adls_censo_accountKey')]"
					},
					"ls_adls_censo_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_censo_properties_typeProperties_url')]"
					},
					"ls_ibge_sidra_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ibge_sidra_http_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}